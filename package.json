{
  "name": "wlmaz",
  "version": "1.0.0",
  "description": "Wiki Loves Monuments Azerbaijan Map",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm run dev:backend\"",
    "dev:frontend": "vite --host",
    "dev:backend": "tsx watch --env-file=.env src/index.ts",
    "update-data": "tsx scripts/update-monuments.ts && pnpm run convert-data",
    "convert-data": "json2geobuf < public/monuments.geojson > public/monuments.pbf",
    "build": "pnpm run convert-data && vue-tsc -b && vite build",
    "preview": "vite preview --host",
    "format": "prettier --write --list-different src/",
    "lint": "eslint src/ --ext .ts,.vue --fix",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,vue,js}": "eslint --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "chart.js": "^4.5.1",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "exifr": "^7.1.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.1",
    "fuse.js": "^7.1.0",
    "geobuf": "^3.0.2",
    "heic2any": "^0.0.4",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "leaflet": "^1.9.4",
    "leaflet-sidebar-v2": "^3.2.3",
    "leaflet.locatecontrol": "^0.85.1",
    "leaflet.markercluster": "^1.5.3",
    "morgan": "^1.10.0",
    "multer": "2.0.2",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^12.1.0",
    "oauth-1.0a": "^2.2.6",
    "passport": "0.7.0",
    "passport-mediawiki-oauth": "^0.1.0",
    "pbf": "^3.2.1",
    "pinia": "^3.0.3",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.15.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/compression": "^1.8.1",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/geobuf": "^3.0.4",
    "@types/geojson": "^7946.0.16",
    "@types/hpp": "^0.2.7",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet.locatecontrol": "^0.82.1",
    "@types/leaflet.markercluster": "^1.5.6",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.0",
    "@types/passport": "^1.0.17",
    "@types/pbf": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.0",
    "geojson": "^0.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.1",
    "typescript": "^5.7.3",
    "vite": "^7.1.9",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-pwa": "^1.1.0",
    "vue-tsc": "^3.0.0"
  },
  "pnpm": {
    "overrides": {
      "passport": "0.7.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
